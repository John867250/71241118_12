fname = input("Masukkan nama file: ")
emails = dict()
try:
    with open(fname) as f:
        for line in f:
            if line.startswith("From "):
                words = line.split()
                email = words[1]
                emails[email] = emails.get(email, 0) + 1
except FileNotFoundError:
    print("File tidak ditemukan.")
print(emails)
